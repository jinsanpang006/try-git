ACA,KADA,EACA,KADA,mBACA,KADA;AAAA;AAAA,uBAEA,SAAA,CAAA;AAAA,kBAAA,IAAA,EAAA,IAAA;AAAA,kBAAA,KAAA,EAAA,KAAA;AAAA,kBAAA,KAAA,EAAA;AAAA,iBAAA,CAFA;;AAAA;AAEA,gBAAA,GAFA;AAGA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA,EAJA,CAKA;;;AACA,gBAAA,MAAA,CAAA,SAAA,CAAA,KAAA,CAAA,YAAA,EAAA,IAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KAfA;AAgBA,IAAA,WAhBA,uBAgBA,GAhBA,EAgBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AAEA,gBAAA,QAJA,GAIA,IAAA,QAAA,EAJA;AAMA,gBAAA,QAAA,CAAA,MAAA,CAAA,OAAA,EAAA,GAAA,CAAA,IAAA;AANA;AAAA,uBAQA,eAAA,CAAA,QAAA,CARA;;AAAA;AAQA,gBAAA,GARA;AASA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA;;AACA,gBAAA,MAAA,CAAA,SAAA,CAAA,KAAA,CAAA,aAAA,EAAA,MAAA,CAAA,QAAA,CAAA,KAAA;;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;;AAZA;AAAA;;AAAA;AAAA;AAAA;AAcA,gBAAA,OAAA,CAAA,GAAA;;AAdA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBA;AAhCA;AAtBA,CAAA","sourcesContent":["<template>\r\n  <div class='settings-container'>\r\n    <el-card>\r\n      <!-- 导航 -->\r\n      <div slot=\"header\">\r\n        <my-breadcrumb>个人设置</my-breadcrumb>\r\n      </div>\r\n      <!-- 栅格 -->\r\n      <el-row>\r\n        <el-col :span=\"12\">\r\n          <!-- 表单 -->\r\n          <el-form label-width=\"120px\">\r\n            <el-form-item label=\"编号：\">100</el-form-item>\r\n            <el-form-item label=\"手机：\">{{userInfo.mobile}}</el-form-item>\r\n            <el-form-item label=\"媒体名称：\">\r\n              <el-input v-model=\"userInfo.name\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"媒体简介：\">\r\n              <el-input v-model=\"userInfo.intro\" type=\"textarea\" :rows=\"3\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"联系邮箱：\">\r\n              <el-input v-model=\"userInfo.email\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item>\r\n              <el-button type=\"primary\" @click=\"save\">保存设置</el-button>\r\n            </el-form-item>\r\n          </el-form>\r\n        </el-col>\r\n        <el-col :span=\"12\">\r\n          <!-- 上传组件 action必须属性-->\r\n          <el-upload\r\n            class=\"avatar-uploader\"\r\n            action=\"\"\r\n            :show-file-list=\"false\"\r\n            :http-request='uploadPhoto'>\r\n            <img v-if=\"userInfo.photo\" :src=\"userInfo.photo\" class=\"avatar\">\r\n            <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n          </el-upload>\r\n          <p class=\"edit-photo\">修改头像</p>\r\n        </el-col>\r\n      </el-row>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport MyBreadcrumb from '@/components/my-breadcrumb.vue'\r\nimport { Info, userPatch, reqModUserImage } from '@/api/user.js'\r\nexport default {\r\n  name: 'Settings',\r\n  components: {\r\n    MyBreadcrumb\r\n  },\r\n  data () {\r\n    return {\r\n      // 用户信息\r\n      userInfo: {\r\n        name: 'test',\r\n        intro: 'info',\r\n        email: 'info@qq.com',\r\n        mobile: '17788889999',\r\n        photo: ''\r\n      },\r\n      // 支持上传组件的数据\r\n      imageUrl: null\r\n    }\r\n  },\r\n  created () {\r\n    this.loadUserInfo()\r\n  },\r\n  methods: {\r\n    async loadUserInfo () {\r\n      const res = await Info()\r\n      console.log(res)\r\n      this.userInfo = res.data.data\r\n    },\r\n    // 修改用户资料\r\n\r\n    async save () {\r\n      const { name, email, intro } = this.userInfo\r\n      const res = await userPatch({ name, email, intro })\r\n      console.log(res)\r\n      this.$message.success('修改成功')\r\n      // 传给layout的created 显示\r\n      this.$eventBus.$emit('uploadName', name)\r\n    },\r\n    async uploadPhoto (obj) {\r\n      try {\r\n        console.log(obj)\r\n\r\n        const formData = new FormData()\r\n\r\n        formData.append('photo', obj.file)\r\n\r\n        const res = await reqModUserImage(formData)\r\n        console.log(res)\r\n        this.userInfo.photo = res.data.data.photo\r\n        this.$eventBus.$emit('uploadImage', this.userInfo.photo)\r\n        this.$message.success('上传头像成功')\r\n      } catch (err) {\r\n        console.log(err)\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped lang='less'>\r\n.avatar{\r\n  width: 178px;\r\n  height: 178px;\r\n  line-height: 178px;\r\n}\r\n.avatar-uploader {\r\n    text-align: center;\r\n}\r\n.avatar-uploader-icon {\r\n    font-size: 28px;\r\n    color: #8c939d;\r\n    width: 178px;\r\n    height: 178px;\r\n    line-height: 178px;\r\n    text-align: center;\r\n}\r\n.avatar-uploader /deep/ .el-upload {\r\n    border: 1px dashed #d9d9d9;\r\n    border-radius: 6px;\r\n    cursor: pointer;\r\n    position: relative;\r\n    overflow: hidden;\r\n}\r\n.edit-photo{\r\n  font-size: 12px;\r\n  text-align: center;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/settings"}]}